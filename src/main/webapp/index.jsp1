<script type="text/javascript">
	$(function(){
		/* if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
			window.location = "${pageContext.request.contextPath}/mobile/index.html"; 
