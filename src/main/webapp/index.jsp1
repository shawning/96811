        $(".carousel").carousel({
            interval: 8000
        });
        $('li.dropdown').mouseover(function() {   
     		$(this).addClass('open');    
     	}).mouseout(function() {
     		$(this).removeClass('open');    
     	}); 
     	$(".ul_icon>li").mouseover(function (){  
			var Obj = $(this).attr('id');
			  num = Obj.substring(2, Obj.length);
            $("#tips"+num).show();  
        }).mouseout(function (){  
            $(".ul_icon li>div[id]").hide();  
        });
    });
